{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "zone-game debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:local", "--", "--inspect-brk"],
      "console": "integratedTerminal",
      "restart": true,
      "autoAttachChildProcesses": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "zone-game-worker debug",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "worker:start:local", "--", "--inspect-brk"],
      "console": "integratedTerminal",
      "restart": true,
      "autoAttachChildProcesses": true
    }
  ]
}
